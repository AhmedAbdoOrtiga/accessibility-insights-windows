<UserControl x:Class="AccessibilityInsights.SharedUx.Controls.SettingsTabs.WhatsNewTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:whatsnew="clr-namespace:AccessibilityInsights.SharedUx.Controls.WhatsNew"
             xmlns:Properties="clr-namespace:AccessibilityInsights.SharedUx.Properties"
             mc:Ignorable="d" AutomationProperties.Name="{x:Static Properties:Resources.WhatsNewTabControlAutomationPropertiesName}"
             d:DesignHeight="400" d:DesignWidth="400">
    <UserControl.Resources>
        <ResourceDictionary Source="..\..\Resources\Styles.xaml"/>
    </UserControl.Resources>
    <StackPanel>
        <whatsnew:WhatsNewControl Version="Current"/>
    </StackPanel>
</UserControl>
