<!-- Contains theme for windows high contrast mode -->
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush po:Freeze="True" x:Key="HLGray" Color="{x:Static SystemColors.GrayTextColor}"/>
    <SolidColorBrush po:Freeze="True" x:Key="HLBlack" Color="{x:Static SystemColors.ControlTextColor}"/>
</ResourceDictionary>
