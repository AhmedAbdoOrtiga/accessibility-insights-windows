<!-- Contains theme for the default GitHub Extension light theme -->
<ResourceDictionary     
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="HLGray" Color="{x:Static SystemColors.GrayTextColor}"/>
    <SolidColorBrush x:Key="HLBlack" Color="{x:Static SystemColors.ControlTextColor}"/>
</ResourceDictionary>
